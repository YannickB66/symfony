{% extends 'basekgb.html.twig' %}

{% block title %}Les missions{% endblock %}

{% block body %}

    <div class="container-lg text-center p-5">
        <h1>
            LES MISSIONS
        </h1>
    </div>

    <div class="container-lg text-center font d1">
        <div class="grid-name row">
            <div class="col border ">
                Nom de l'agent
            </div>
            <div class="col border">
                prenom de l'agent
            </div>
            <div class="col border">
                spécialité de l'agent
            </div>
        </div>

        {% for agent in agents %}

        <div class="row">
            <div class="col border">
                {{ agent.name }}
            </div>
            <div class="col border">
                {{ agent.firstName}}
            </div>
            <div class="col border">
                spec
            </div>
        </div>

        {% endfor %}

    </div>


{% endblock %}
